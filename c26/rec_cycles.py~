def cycles(n, d):
  x = n * 9
  z = x
  k = 1
  while z % d:
    z = z * 10 + x
    k += 1
  return k


assert cycles(1, 7) == 6
assert cycles(1, 3) == 1

hd = 1
lc = 0

for de in range(1, 10):
  print(de)
  
  c = cycles(1, de)

  if c > lc and c != 0:
    lc = c
    hd = d
    
print(f"\n\n{hd}\n\n√π")
